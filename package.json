{
  "name": "jira-modal-tamer-extension",
  "version": "0.0.1",
  "description": "Tame JIRA modal windows, allowing you to drag and peek them.",
  "browser": "dist/inject.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup --config",
    "zip": "cross-var zip -r -j dist/$npm_package_name-$npm_package_version.zip dist/*",
    "dist": "npm run build && npm run zip"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/nandastone/jira-modal-tamer-extension.git"
  },
  "author": "Matt Stone",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nandastone/jira-modal-tamer-extension/issues"
  },
  "homepage": "https://github.com/nandastone/jira-modal-tamer-extension#readme",
  "dependencies": {
    "cross-var": "^1.1.0",
    "interactjs": "^1.9.20"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "eslint": "^7.6.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-zip": "^1.0.1"
  }
}
